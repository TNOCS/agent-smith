{
    "name": "battle-lab-dashboard",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "install": "npm i",
        "serve": "vue-cli-service serve --watch --inline-vue",
        "build": "vue-cli-service build --dest --inline-vue ../copper-server/dist/dashboard",
        "build:watch": "vue-cli-service build --watch --dest ../copper-server/dist/dashboard",
        "bundle-report": "webpack-bundle-analyzer --port 4200 dist/stats.json",
        "clean": "del-cli package-lock.json dist lib node_modules tsconfig.tsbuildinfo",
        "dev": "pnpm build:watch",
        "linkcs": "pnpm link @csnext/cs-client && pnpm link @csnext/cs-split-panel && pnpm link @csnext/cs-map && pnpm link @csnext/cs-form && pnpm link @csnext/cs-timeline && pnpm link @csnext/cs-data",
        "unlinkcs": "pnpm unlink @csnext/cs-core && pnpm unlink @csnext/cs-client && pnpm unlink @csnext/cs-split-panel && pnpm unlink @csnext/cs-map && pnpm unlink @csnext/cs-form && pnpm unlink @csnext/cs-timeline && pnpm unlink @csnext/cs-data",
        "update": "pnpm install && pnpm linkcs"
    },
    "dependencies": {
        "@csnext/cs-client": "canary",
        "@csnext/cs-core": "canary",
        "@csnext/cs-form": "canary",
        "@csnext/cs-map": "canary",
        "@csnext/cs-data": "canary",
        "axios": "~0.20.0",
        "chroma-js": "^2.1.0",
        "class-transformer": "^0.2.3",
        "date-fns": "^2.16.1",
        "geojson": "^0.5.0",
        "milsymbol": "2.0.0",
        "source-map-support": "^0.5.19",
        "vue": "^2.6.12",
        "vue-class-component": "^7.2.5",
        "vue-markdown": "^2.2.4",
        "vue-property-decorator": "^9.0.0",
        "babel-loader": "^8.1.0",
        "@mapbox/mapbox-gl-draw": "1.2.0",
        "@mapbox/mapbox-gl-geocoder": "^4.7.0",
        "@mapbox/mapbox-gl-traffic": "1.0.2",
        "reflect-metadata": "0.1.13",
        "simplebar-vue": "1.5.1",
        "socket.io-client": "2.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.11.6",
        "@types/chroma-js": "^2.0.0",
        "@types/d3": "5.7.2",
        "@types/lodash.throttle": "^4.1.6",
        "@types/mapbox-gl": "^1.11.2",
        "@types/node": "^14.6.4",
        "@types/socket.io-client": "^1.4.33",
        "@vue/babel-preset-app": "^4.5.5",
        "@vue/cli-plugin-babel": "^4.5.5",
        "@vue/cli-plugin-typescript": "^4.5.5",
        "@vue/cli-service": "^4.5.5",
        "babel-runtime": "^6.26.0",
        "core-js": "3.6.5",
        "dojo-typings": "^1.11.11",
        "fibers": "^5.0.0",
        "html-loader": "^1.3.0",
        "sass": "1.26.10",
        "sass-loader": "^10.0.2",
        "typescript": "^4.0.2",
        "vue-cli-plugin-pwa": "^4.6.0",
        "vue-template-compiler": "^2.6.12",
        "webpack-bundle-analyzer": "^3.8.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ],
    "build": {
        "files": [
            "dist/**/*",
            "node_modules/**/*",
            "package.json",
            "dist/**/*",
            "node_modules/**/*",
            "package.json"
        ]
    }
}
