{
    "name": "copper-server",
    "version": "0.0.1",
    "description": "",
    "main": "server.js",
    "scripts": {
        "start": "node dist/index.js",
        "start:build": "node dist/index.js",
        "debug": "cross-env TZ=UTC NODE_ENV=development tsnd --inspect --respawn src/index.ts",
        "dev:server": "ts-node-dev src/index.ts",
        "dev": "ts-node-dev src/index.ts",
        "build:server": "node node_modules/typescript/bin/tsc",
        "build": "tsc",
        "build:package": "pkg dist/index.js --output dist/dd.exe --targets node10-win-x64",
        "linkcs": "yarn link @csnext/cs-layer-server",
        "unlinkcs": "yarn unlink @csnext/cs-layer-server",
        "watch": "tsc -w",
        "clean": "del-cli dist node_modules yarn.lock tsconfig.tsbuildinfo",
        "dev:run": "ts-node-dev index.ts",
        "test": "echo \"Error: no test specified\" && exit 1",
        "init": "npm i"
    },
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@csnext/cs-layer-server": "canary",
        "@mapbox/togeojson": "^0.16.0",
