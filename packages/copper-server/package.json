        "@nestjs/core": "^7.4.4",
        "@nestjs/cqrs": "7.0.1",
        "class-transformer": "^0.3.1",
        "date-fns": "^2.16.1",
        "@nestjs/swagger": "^4.6.0",
        "cross-env": "7.0.2",
        "@nestjs/typeorm": "^7.1.4",
        "@types/command-line-args": "^5.0.0",
        "@types/node": "^14.10.3",
        "command-line-args": "^5.1.1",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "fs-extra": "^9.0.1",
        "nestjs": "^0.0.1",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^6.6.3",
        "typeorm": "^0.2.26",
        "lodash": "^4.17.20",
        "protobufjs": "^6.10.1",
        "peer": "0.5.3",
        "pm2": "4.4.1",
        "node-test-bed-adapter": "^2.0.8",
        "test-bed-schemas": "^0.3.8"
    },
    "devDependencies": {
        "@types/dotenv": "^8.2.0",
